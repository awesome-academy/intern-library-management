
<%= form_tag "/search", class: "form-search-admin", id: "form-search-room-admin", method: :get do %>
  <div class="box-input-search">
    <%= text_field_tag :name, params[:name], class: "form-control input-search",
        placeholder: "sasasasasas" %>
  </div>
  <div class="box-submit">
    <%= submit_tag t("searchs.search"), class: "btn btn-primary btn-login" %>
  </div>
<% end %>
